<service>
    <request> <!-- Specifies how to retrieve/access service -->
        <url>
            http://dl.acm.org/results.cfm?coll=DL&amp;dl=GUIDE&amp;CFID={{{cfid}}}&amp;CFTOKEN={{{cftoken}}}
        </url>
        <method>POST</method> <!-- HTTP Method: GET, POST, etc. -->
        <body contentType="application/x-www-form-urlencoded"> <!-- POST parameters -->
            <item>
                <name>query</name>
                <value>{{query}}</value>
            </item>
        </body>
        <cookies>
            <item>
                <name>CFID</name>
                <value>{{cfid}}</value>
            </item>
            <item>
                <name>CFTOKEN</name>
                <value>{{cftoken}}</value>
            </item>
            <item>
                <name>DEEPCHK</name>
                <value>1</value>
            </item>
        </cookies>
    </request>
    <response contentType="text/html">

        <meta>
            <count></count>
        </meta>

        <articles>

        </articles>

    </response>
</service>