<service>
    <request> <!-- Specifies how to retrieve/access service -->
        <url>http://dl.acm.org/</url>
        <method>GET</method> <!-- HTTP Method: GET, POST, etc. -->
    </request>
    <response contentType="text/html">
        <!-- from="cookie" searches in returned cookies; mode="text" in that case means cookie value. -->
        <cfid from="cookie" el="CFID" mode="text" />
        <cftoken from="cookie" el="CFTOKEN" mode="text" />
    </response>

